{
  "version": 2,
  "builds": [
    {
      "src": "mynetcrud/wsgi.py", //--> enter project name
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mynetcrud/wsgi.py"   //--> enter project name
    }
  ],
  "outputDirectory": "static"
}